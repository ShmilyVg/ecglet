
.bottom-text{
    font-size:28rpx;
    font-weight:400;
    color:white;
    line-height:40rpx;
    margin-bottom: 200rpx;
    margin-top: 20rpx;
    text-align: center;
}

.result-advice{
    font-size:40rpx;
    font-family:PingFang-SC-Medium;
    font-weight:500;
    color:rgba(255,254,254,1);
    margin: 59rpx 0 41rpx 50rpx;
    align-self: flex-start;
}
.icon-common-layout{
    position: relative;
    width: 70rpx;
    height: 64rpx;
    margin-bottom: 35rpx;
}
.icon-common{
    width: 70rpx;
    height: 64rpx;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 1;
}

.icon-common-bg{
    margin: 2rpx;
    width: calc(100% - 4rpx);
    height: calc(100% - 4rpx);
}

.common-advice-circle{
    width:11rpx;
    height:11rpx;
    background:rgba(0,198,188,1);
    border-radius: 50%;
    margin-top: 15rpx;
    margin-right: 13rpx;
}

.advice-content{
    font-size:28rpx;
    font-family:PingFang-SC-Regular;
    font-weight:400;
    color:rgba(91,91,91,1);
}

.advice-item-layout{
    width: 40%;
    padding: 50rpx 0 40rpx 40rpx;
    align-items: flex-start;
}


.cell-bottom-view{
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 10;
    background-color: white;
    height: 120rpx;
    width: 100%;
    line-height: 120rpx;
    text-align: center;
    font-size:38rpx;
    font-family:PingFang-SC-Medium;
    font-weight:500;
    color:rgba(58,147,239,1);
}

.table-02{
    background-color: white;
    padding-top: 50rpx;
    border-radius: 30rpx;
    width:91.6%;
    z-index: 1;
}

.result-bottom-remind{
    width:626rpx;
    font-size:26rpx;
    font-family:PingFang-SC-Medium;
    font-weight:500;
    margin-top: 46rpx;
    margin-bottom: 76rpx;
    line-height: 46rpx;
}

