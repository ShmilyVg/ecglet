.logo {
    width: 80rpx;
    height: 80rpx;
    margin-top: 25rpx;
    z-index: 5;
    position: relative;
    border-radius: 50%;
    border-width: 15rpx;
    border-style: solid;
}
.table{
    background-color: white;
    margin-top: -50rpx;
    border-radius: 30rpx;
    width:91.6%;
    z-index: 1;
    box-shadow:0 8rpx 20rpx 0 rgba(0,190,180,0.3);
}

.cell{
    padding: 42rpx 0 30rpx 50rpx;
}
.outer-circle{
    border-radius: 50%;
    width: 42rpx;
    height: 42rpx;
    margin-right: 33rpx;
}
.point-comm{
    border-radius: 50%;
    width: 100%;
    height: 100%;
    z-index: 2;
}

.point-red{
    border-color: #F9B0BD;
    background-color: #EF3A59;
}

.point-blue{
    border-color: #B0D4F9;
    background-color: #3A93EF;
}

.point-green{
    border-color: #00f9ee;
    background-color: #00C6BC;
}

.cell-title {
    font-size:32rpx;
    font-weight:400;
    color:rgba(91,91,91,1);
    line-height:56rpx;
}
.cell-green-state{
    width:246rpx;
    font-size:24rpx;
    line-height:32rpx;
    font-family:PingFang-SC-Regular;
    font-weight:400;
    color:rgba(156,156,156,1);
}
.cell-state {
    font-size:26rpx;
    font-weight:400;
    line-height:37rpx;
}

.cell-red{
    color:rgba(239,58,89,1);
}


.cell-num {
    font-size:44rpx;
    font-weight:500;
    font-family:DINCondensed-Bold;
    margin-bottom: -10rpx;
}

.cell-eng {
    font-size:24rpx;
    font-weight:400;
    color:rgba(158,158,158,1);
    line-height:33rpx;
}

.result-item-extra{
    padding: 34rpx;
    margin-right: 10rpx;
}

.result-item-arrow{
    width: 44rpx;
    height: 44rpx;
}

.result-item-detail{
    width:576rpx;
    font-size:28rpx;
    font-family:PingFang-SC-Regular;
    font-weight:400;
    color:rgba(91,91,91,1);
    margin-bottom: 48rpx;
    align-self: center;
}

.result-item-line{
    width:calc(100% - 8rpx);
    border: 1rpx solid #E8E8E8;
}

.display{
    display: block;
}

.hidden{
    display: none;
}

.cell-blue{
    color:#3A93EF;
}
.cell-green{
    color: #00C6BC;
}
