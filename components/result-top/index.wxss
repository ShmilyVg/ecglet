.table{
}

.cell{
    padding: 32rpx 0 32rpx 89rpx;
}

.cell-title {
    font-size:36rpx;
    font-family:PingFang-SC-Bold;
    font-weight:bold;
    color: #33444E;
    line-height:28rpx;
    margin-top: 16rpx;
}

.cell-green-state{
    width:246rpx;
    font-size:24rpx;
    line-height:32rpx;
    font-family:PingFang-SC-Regular;
    font-weight:400;
    color:rgba(156,156,156,1);
}
.cell-state {
    font-size:28rpx;
    font-family:PingFang-SC-Regular;
    font-weight:400;
    color:rgba(113,142,160,1);
    line-height:32rpx;
}

.cell-red{
    color:rgba(239,58,89,1);
}

.cell-num {
    font-size:70rpx;
    font-family:DINCondensed-Bold;
    font-weight:bold;
    color:rgba(97,118,131,1);
    line-height:84rpx;
}

.cell-eng {
    font-size:28rpx;
    font-family:PingFang-SC-Regular;
    font-weight:400;
    color:rgba(97,118,131,1);
    line-height:32rpx;
    margin-top: -15rpx;
}

.result-item-extra{
    padding: 34rpx;
    margin-right: 10rpx;
}

.result-item-arrow{
    width: 44rpx;
    height: 44rpx;
}

.result-item-detail{
    width:576rpx;
    font-size:28rpx;
    line-height:40rpx;
    font-family:PingFang-SC-Regular;
    font-weight:400;
    color:rgba(91,91,91,1);
    margin-bottom: 48rpx;
    align-self: center;
    margin-right: 50rpx;
}

.result-item-line{
    width: 748rpx;
    border-bottom: 1rpx solid #F6F7F7;
}

.display{
    display: block;
}

.hidden{
    display: none;
}

.cell-blue{
    color:#3A93EF;
}
.cell-green{
    color: #00C6BC;
}
