<view class="page" xmlns:wx="http://www.w3.org/1999/xhtml">
  <view class="weui-cells__title">资料</view>
  <view class="weui-cells weui-cells_after-title">
    <block>
      <navigator url="../userdata/userdata" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
        <view class="weui-cell__bd">用户信息</view>
        <view class="weui-cell__ft weui-cell__ft_in-access">{{userData.first_name}}</view>
      </navigator>
    </block>
  </view>
  <view class="weui-cells__title">其他</view>
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell weui-cell_access" hover-class="weui-cell_active" bindtap="call">
      <view class="weui-cell__bd">联系我们</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">{{phone}}</view>
    </view>
  </view>
</view>
