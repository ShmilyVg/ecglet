@import "../../components/result-top/index.wxss";
@import "../../components/result-bottom/index.wxss";
page {
  background-color: transparent;

}

.result-top-layout{
  height: 500rpx;
  position: relative;
  overflow: hidden;
}

.result-top-layout::after {
  content: '';
  width: 180%;
  height: 420rpx;
  position: absolute;
  z-index: -1;
  left: -40%;
  top: 0;
  border-radius: 0 0 50% 50%;
  background-color: #3A93EF;
}

.result-top-image-bg{
  background-image: url("data:image/png;base64,");
  background-position: top center;
  background-size: contain;
  background-repeat: no-repeat;
  padding: 0 40rpx 0 59rpx;
  margin-top: 50rpx;
}

.result-title{
  font-size:44rpx;
  font-family:PingFang-SC-Bold;
  font-weight:bold;
  color:rgba(255,255,255,1);
}

.result-content{
  font-size:28rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
  margin-top: 14rpx;
}

.logo {
  width: 95rpx;
  height: 95rpx;
  position: relative;
  margin-top: -150rpx;
  border-radius: 50%;
  border-width: 15rpx;
  border-style: solid;
  background-color: #3A93EF;
  border-color: #3A93EF;
}

.share{
  position: absolute;
  top: 40rpx;
  right: 30rpx;
  font-size:24rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
  border-radius:35rpx;
  opacity:0.8051;
  border:1rpx solid rgba(255,255,255,1);
  padding: 15rpx 30rpx;
}

.avatar{
  width: 116rpx;
  height: 116rpx;
  border-radius: 50%;
  border: 12rpx solid #48A4FF;
}

.title {
  font-size:40rpx;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:28rpx;
  margin-top: 60rpx;
}

.content {
  font-size:24rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:28rpx;
  margin-top: 22rpx;
}
