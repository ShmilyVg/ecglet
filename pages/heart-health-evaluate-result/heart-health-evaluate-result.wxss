}

.decipher-contents-item-title{
    font-size:28rpx;
    font-family:PingFang-SC-Medium;
    font-weight:500;
    color:rgba(91,91,91,1);
    line-height:42rpx;
    margin-left: 13rpx;
}
.decipher-contents-item-content{
    font-size:28rpx;
    font-family:PingFang-SC-Regular;
