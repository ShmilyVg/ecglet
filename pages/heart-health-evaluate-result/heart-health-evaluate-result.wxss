.heart-health-evaluate-result-title{
    font-size:36rpx;
    font-family:PingFang SC;
    font-weight:500;
    color:rgba(51,51,51,1);
    line-height:50rpx;
    margin: 0 0 41rpx;
}

.heart-health-evaluate-result-level{
    font-size:120rpx;
    font-family:PingFang SC;
    font-weight:800;
    color:rgba(97,118,131,1);
    line-height:50rpx;
    margin-left: 30rpx;
    margin-right: 13rpx;

}

.heart-health-evaluate-result-level-sub{
    font-size:32rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(91,91,91,1);
    line-height:50rpx;
}

.heart-health-evaluate-result-level-item{
    padding: 12rpx 55rpx;
    font-size:26rpx;
    font-family:PingFang SC;
    font-weight:500;
    line-height:50rpx;
    border-radius: 10rpx;
    border-style: solid;
    border-width: 1rpx;
}

.heart-health-evaluate-result-level-item:nth-child(2){
    margin: 0 12rpx;
}

.heart-health-evaluate-result-level-description{
    font-size:28rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(91,91,91,1);
    line-height:46rpx;
    text-align: center;
    margin-top: 58rpx;
    padding: 0 67rpx;
}

.heart-health-evaluate-result-line{
    width:100%;
    height:20rpx;
    background:rgba(246,247,249,1);
    margin-top: 88rpx;
    margin-bottom: 76rpx;
}


.heart-health-evaluate-result-dotted-line{
    width:612rpx;
    border: 1px dotted rgba(156,156,156,1);
    margin-top: 44rpx;
    margin-bottom: 51rpx;
}

.heart-health-evaluate-result-final{
    width: 460rpx;
    font-size:38rpx;
    font-family:PingFang SC;
    font-weight:500;
    color:rgba(91,91,91,1);
    line-height:50rpx;
    text-align: center;
    margin:0 24rpx;
}

.heart-health-evaluate-result-final-compare{
    font-size:28rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(114,101,227,1);
    line-height:46rpx;
    padding: 18rpx 30rpx;
    margin-top: 48rpx;
    background:rgba(237,234,255,1);
    border-radius:10rpx;
}

.heart-health-evaluate-result-content{
    width: 642rpx;
    font-size:28rpx;
    font-family:PingFang SC;
    font-weight:400;
    color:rgba(91,91,91,1);
    line-height:46rpx;
    margin-top: 28rpx;
}

.decipher-item-icon{
    width: 34rpx;
    height: 36rpx;
}

.decipher-content-title{
    font-size:32rpx;
    font-family:PingFang-SC-Medium;
    font-weight:500;
    color:rgba(51,68,78,1);
    line-height:42rpx;
    margin-left: 13rpx;
}
.decipher-suggest-item-title{
    font-size:28rpx;
    font-family:PingFang-SC-Medium;
